{
  "name": "ember-meta-explorer",
  "version": "0.0.5",
  "description": "Statically analyzes ember project source for dependency trees and other information.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --no-watchman",
    "clean": "rm -rf dist; mkdir dist",
    "build": "tsc -p tsconfig.build.json",
    "tsconf": "tsc --init --declaration --allowSyntheticDefaultImports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lifeart/ember-meta-explorer.git"
  },
  "keywords": [
    "glimmer",
    "ember",
    "static",
    "analysis",
    "handlebars"
  ],
  "author": "Aleksandr Kanunnikov <lefeart92@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lifeart/ember-meta-explorer/issues"
  },
  "homepage": "https://github.com/lifeart/ember-meta-explorer#readme",
  "devDependencies": {
    "@types/babel-core": "^6.25.6",
    "@types/debug": "^0.0.30",
    "@types/jest": "^24.0.4",
    "@types/node": "^7.0.18",
    "jest": "^24.1.0"    
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@glimmer/syntax": "^0.39.2",
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "typescript": "~2.4.0"
  }
}
